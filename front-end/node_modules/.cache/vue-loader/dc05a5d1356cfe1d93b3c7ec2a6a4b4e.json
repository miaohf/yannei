   this.$store\r\n          .dispatch('user/login', this.loginForm)\r\n          .then(() => {\r\n            this.$router.push({\r\n              path: this.redirect || '/',\r\n              query: this.otherQuery,\r\n            })\r\n            this.$store.dispatch('user/getInfo')\r\n            this.loading = false\r\n          })\r\n          .catch(() => {\r\n            this.loading = false\r\n          })\r\n      )\r\n    },\r\n    reset() {\r\n      this.$refs.form.reset()\r\n    },\r\n    resetValidation() {\r\n      this.$refs.form.resetValidation()\r\n    },\r\n  },\r\n}\r\n</script>\r\n"]}]}